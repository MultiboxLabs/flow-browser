{
  "name": "flow-browser",
  "productName": "Flow",
  "version": "0.6.4",
  "description": "A modern privacy-focused browser with a minimalistic design.",
  "main": "./out/main/index.js",
  "author": {
    "name": "iamEvan",
    "email": "evan@iamevan.dev",
    "url": "https://iamevan.dev"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "start:nightly": "bun run scripts/temp-change-name/command --name 'Flow Nightly' electron-vite preview",
    "dev": "electron-vite dev",
    "dev:watch": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@ghostery/adblocker-electron": "^2.5.2",
    "@phosphor-icons/core": "^2.1.1",
    "better-sqlite3": "^11.9.1",
    "electron-chrome-extensions": "npm:@iamevan/electron-chrome-extensions@4.7.5",
    "electron-chrome-web-store": "npm:@iamevan/electron-chrome-web-store@0.11.2",
    "electron-context-menu": "^4.0.5",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "ico-to-png": "^0.2.2",
    "knex": "^3.1.0",
    "mime-types": "^3.0.1",
    "posthog-node": "^4.17.1",
    "sharp": "^0.34.1",
    "sharp-ico": "^0.1.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@phosphor-icons/react": "^2.1.7",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.3",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.8",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@types/chrome": "^0.0.317",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "lucide-react": "^0.503.0",
    "motion": "^12.7.4",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sonner": "^2.0.3",
    "string-similarity-js": "^2.1.4",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.7",
    "typescript": "^5.8.3",
    "vite": "^6.2.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  },
  "trustedDependencies": [
    "lodepng"
  ]
}
